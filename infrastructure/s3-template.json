{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "CloudFormation template to create S3 bucket for storing application binaries.",
  "Resources": {
    "ApplicationBucket": {
      "Type": "AWS::S3::Bucket",
      "Properties": {
        "BucketName": "fabletop-games-bucket",
        "VersioningConfiguration": {
          "Status": "Enabled"
        }
      }
    }
  }
}
